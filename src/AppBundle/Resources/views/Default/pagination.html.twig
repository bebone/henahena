<div class="row">

    {% if pagination.nbPages > 0 %}
        <ul class="pagination">
            {% if pagination.page > 1 %}
                <li id="1">
                    <a href="#">
                        <<
                    </a>
                </li>
                <li id="{{pagination.page-1 }}">
                    <a href="#">
                        <
                    </a>
                </li>
            {% endif %}


            {% for p in range(max(pagination.page-4, 1), min(pagination.page+4, pagination.nbPages)) %}
                <li id="{{ p }}" {% if p == pagination.page %}class="active"{% endif %}>
                    <a href="#">
                        {{ p }}
                    </a>
                </li>
            {% endfor %}

            {% if pagination.page < pagination.nbPages %}
                <li id="{{pagination.page+1}}">
                    <a href="#">
                        >
                    </a>
                </li>
                <li id="{{ pagination.nbPages }}">
                    <a href="#">
                        >>
                    </a>
                </li>
            {% endif %}
        </ul>
    {% endif %}
</div>